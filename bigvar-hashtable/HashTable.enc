
module HashTable

local class HashEntry[k,v]
    var key : k
    var value : v
    var hasEntry : bool

    def init() : unit
        this.hasEntry = false
    end

    def add(key:k,value:v) : unit
        this.hasEntry = true
        this.key = key
        this.value = value
    end

    def notEmpty() : bool
        this.hasEntry
    end

    def getKey() : k
        this.key
    end

    def getValues() : v
        this.value
    end
end
