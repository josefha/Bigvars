
import Bighash


active class Main
    def main() : unit
        println("Hello bighash")

        var hashtable = new Bighash[String, int]()

        var key1 = "key123"
        hashtable.put(key1,11111)
        hashtable.put(key1,22222)
        println("key1 value = {}",hashtable.get(key1))

        var p1 = new Person("Josef",21)
        var p2 = new Person("Sahand",17)
        var p3 = new Person("Liza",17)
        var hashtable = new Bighash[int,Person]()
        hashtable.put(111,p1)
        hashtable.put(222,p2)
        hashtable.put(333,p3)

        var people = hashtable.elements()

        --var people = new[Person](3)
        --people = hashtable.getMany([111,222,333])

        for person <- people do
            println(person.getname())
        end

    end
end

read class Person
    val name : String
    val age : int
    def init(name:String, age:int) : unit
        this.name = name
        this.age = age
    end

    def getname() : String
        this.name
    end
end
