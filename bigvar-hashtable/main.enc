
import Bighash


active class Main
    def main() : unit
        println("Hello bighash")

        var hashtable = new Bighash[String, int]()

        var key1 = "key123"
        hashtable.put(key1,11111)
        hashtable.put(key1,22222)
        println("key1 value = {}",hashtable.get(key1))

        var p1 = new Person("Josef",21)
        var p2 = new Person("Sahand",17)
        var hashtable = new Bighash[int,Person]()
        hashtable.put(111,p1)
        hashtable.put(123213,p2)
        println("value = {}",hashtable.get(123213).getname())

    end
end

read class Person
    val name : String
    val age : int
    def init(name:String, age:int) : unit
        this.name = name
        this.age = age
    end

    def getname() : String
        this.name
    end
end
