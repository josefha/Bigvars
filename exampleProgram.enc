


active class Main
    def main() : unit
        var person = new Person("Josef", 22)
        var print = new PrintPerson()
        print ! print(consume person)
    end
end

local class Person
    var name : String
    var age : int
    def init(name:String, age:int)
        this.name = name
        this.age = age
    end
end

active class PrintPerson
    def (person:Person)
        print(person.name)
    end
end
